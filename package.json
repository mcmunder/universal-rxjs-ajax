  "description": "Makes rxjs's ajax available in Node.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcmunder/universal-rxjs-ajax.git"
  },
  "keywords": ["rxjs", "observable", "ajax", "universal", "isomorphic"],
